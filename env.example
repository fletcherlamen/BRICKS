# I PROACTIVE BRICK Orchestration Intelligence - Environment Variables

# Application Settings
APP_NAME="I PROACTIVE BRICK Orchestration Intelligence"
APP_VERSION="1.0.0"
DEBUG=true
ENVIRONMENT=development

# API Keys - Add your actual keys to .env file
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here
GOOGLE_GEMINI_API_KEY=your_google_gemini_api_key_here

# Database Configuration
DATABASE_URL=postgresql://user:password@localhost:5432/brick_orchestration
POSTGRES_USER=user
POSTGRES_PASSWORD=password
POSTGRES_DB=brick_orchestration

# Redis Configuration (for caching and session management)
REDIS_URL=redis://localhost:6379

# CrewAI Configuration
CREWAI_API_KEY=your_crewai_api_key_here
CREWAI_BASE_URL=https://api.crewai.com

# Mem0.ai Configuration
MEM0_API_KEY=your_mem0_api_key_here
MEM0_BASE_URL=https://api.mem0.ai

# Devin AI Configuration
DEVIN_API_KEY=your_devin_api_key_here
DEVIN_BASE_URL=https://api.devin.ai

# Microsoft Copilot Studio Configuration
COPILOT_STUDIO_API_KEY=your_copilot_studio_api_key_here
COPILOT_STUDIO_BASE_URL=https://api.copilot.microsoft.com

# GitHub Copilot Configuration
GITHUB_COPILOT_TOKEN=your_github_copilot_token_here

# Security
SECRET_KEY=your_secret_key_for_jwt_and_encryption
JWT_SECRET_KEY=your_jwt_secret_key
JWT_ALGORITHM=HS256
JWT_ACCESS_TOKEN_EXPIRE_MINUTES=30

# CORS Settings
CORS_ORIGINS=["http://localhost:3000", "http://localhost:8000"]

# Logging
LOG_LEVEL=INFO
LOG_FORMAT=json

# Monitoring
ENABLE_METRICS=true
METRICS_PORT=9090

# Business Systems Integration
CHURCH_KIT_API_KEY=your_church_kit_api_key_here
CHURCH_KIT_BASE_URL=https://api.churchkit.com

GLOBAL_SKY_AI_API_KEY=your_global_sky_ai_api_key_here
GLOBAL_SKY_AI_BASE_URL=https://api.globalskyai.com

# Treasury Management
TREASURY_API_KEY=your_treasury_api_key_here
TREASURY_BASE_URL=https://api.treasury.com

# Dream Big Masks Integration
DREAM_BIG_MASKS_API_KEY=your_dream_big_masks_api_key_here
DREAM_BIG_MASKS_BASE_URL=https://api.dreambigmasks.com
