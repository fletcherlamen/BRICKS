# I PROACTIVE BRICK Orchestration Intelligence - Environment Configuration

# Application Settings
APP_NAME="I PROACTIVE BRICK Orchestration Intelligence"
DEBUG=false
LOG_LEVEL=INFO

# API Keys - Multiple AI Providers
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here
GOOGLE_API_KEY=your_google_api_key_here
MEM0_API_KEY=your_mem0_api_key_here

# Database Configuration
DATABASE_URL=postgresql+asyncpg://postgres:password@localhost:5432/brick_orchestration

# CORS Configuration
ALLOWED_ORIGINS=["http://localhost:3000", "http://localhost:3001"]

# CrewAI Configuration
CREWAI_MAX_ITERATIONS=10
CREWAI_VERBOSE=true

# Mem0 Configuration
MEM0_HOST=https://api.mem0.ai
MEM0_PROJECT_ID=brick-orchestration

# Orchestration Settings
MAX_CONCURRENT_TASKS=5
TASK_TIMEOUT=300

# Security
SECRET_KEY=your_secret_key_here
ACCESS_TOKEN_EXPIRE_MINUTES=30
